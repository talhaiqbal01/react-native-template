{
  "expo": {
    "name": "react-native-template",
    "slug": "react-native-template",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/main-logo-dark.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logos/main-logo-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#7cb9e8"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logos/main-logo-dark.png",
        "backgroundColor": "#7cb9e8"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
